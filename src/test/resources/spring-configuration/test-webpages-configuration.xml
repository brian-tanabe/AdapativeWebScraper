<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--================== ESPN TEST WEB PAGES ==================-->
    <bean id="espnNflProjectionsPageOneAsAStringUnformatted"
          class="com.btanabe.adaptivewebscraper.test.factories.FileStringFactory">
        <property name="inputFile"
                  value="classpath:/test-webpages/espn/projection-pages/espn-player-projections-page-1.dirty.html"/>
    </bean>

    <bean id="espnNflProjectionsPageOneAsAStringFormatted"
          class="com.btanabe.adaptivewebscraper.test.factories.FileTidiedStringFactory">
        <property name="inputFile"
                  value="classpath:/test-webpages/espn/projection-pages/espn-player-projections-page-1.dirty.html"/>
    </bean>

    <bean id="espnNflProjectionsPageOneFormatted"
          class="com.btanabe.adaptivewebscraper.test.factories.FileTidiedDocumentFactory">
        <property name="inputFile"
                  value="classpath:/test-webpages/espn/projection-pages/espn-player-projections-page-1.dirty.html"/>
    </bean>

    <bean id="espnProjectionsPageEddieLacyDocument"
          class="com.btanabe.adaptivewebscraper.test.factories.FileTidiedDocumentFactory">
        <property name="inputFile"
                  value="classpath:test-webpages/espn/individual-projection-page-rows/projections-eddie-lacy.html"/>
    </bean>

    <bean id="espnProjectionsPageEddieLacyString"
          class="com.btanabe.adaptivewebscraper.test.factories.FileStringFactory">
        <property name="inputFile"
                  value="classpath:test-webpages/espn/individual-projection-page-rows/projections-eddie-lacy.html"/>
    </bean>

    <bean id="espnProjectionsPageJoshFerguson"
          class="com.btanabe.adaptivewebscraper.test.factories.FileTidiedDocumentFactory">
        <property name="inputFile"
                  value="classpath:test-webpages/espn/individual-projection-page-rows/projections-josh-ferguson.html"/>
    </bean>

    <bean id="espnProjectionsPageSteelersDefense"
          class="com.btanabe.adaptivewebscraper.test.factories.FileTidiedDocumentFactory">
        <property name="inputFile"
                  value="classpath:test-webpages/espn/individual-projection-page-rows/projections-steelers-defense.html"/>
    </bean>

    <!--================== YAHOO TEST WEB PAGES ==================-->
    <bean id="yahooStatsPageAdrianPeterson"
          class="com.btanabe.adaptivewebscraper.test.factories.FileTidiedDocumentFactory">
        <property name="inputFile"
                  value="classpath:test-webpages/yahoo/individual-stat-page-rows/yahoo-player-stats-2015-rb-adrian-peterson.html"/>
    </bean>

    <bean id="yahooStatsPageRunningBacks"
          class="com.btanabe.adaptivewebscraper.test.factories.FileTidiedDocumentFactory">
        <property name="inputFile"
                  value="classpath:test-webpages/yahoo/stat-pages/yahoo-player-stats-2015-rb.html"/>
    </bean>


</beans>